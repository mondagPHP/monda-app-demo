{
  "name": "mondagroup/monda-php",
  "description": "盟大框架",
  "authors": [
    {
      "name": "chenzf",
      "email": "chenzf@pvc123.com"
    }
  ],
  "require": {
    "php": ">=7.2",
    "mondagroup/monda-php-framework": "^1.0",
    "swoole/ide-helper": "~4.4.7"
  },
  "require-dev": {
    "friendsofphp/php-cs-fixer": "^2.14",
    "phpunit/phpunit": "^8.5",
    "mockery/mockery": "^1.3"
  },
  "autoload": {
    "psr-4": {
      "app\\": "app/",
      "tests\\": "tests/"
    },
    "files": [
      "app/function.php"
    ]
  },
  "repositories": {
    "packagist": {
      "type": "composer",
      "url": "https://mirrors.aliyun.com/composer/"
    }
  },
  "scripts": {
    "cs-fix": "composer dump -o && php-cs-fixer fix $1"
  }
}
